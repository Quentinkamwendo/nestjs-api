{
    "version": 2,
    "name": "api",
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/main.ts",
            "methods": ["GET", "POST", "PUT", "DELETE"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ],
    "outputDirectory": "dist"
}